{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "blog"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "data": {
      "type": "datetime"
    },
    "hero": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "categoria": {
      "via": "blogs",
      "model": "categoria"
    }
  }
}
